{
    "printWidth": 140,
    "singleQuote": true,
    "tabWidth": 4,
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "tabWidth": 4,
                "printWidth": 250,
                "parser": "json"
            }
        },
        {
            "files": "*.html",
            "options": {
                "printWidth": 80,
                "tabWidth": 2,
                "trailingComma": "all",
                "htmlWhitespaceSensitivity": "ignore",
                "parser": "html"
            }
        }
    ],
    "parser": "typescript"
}
